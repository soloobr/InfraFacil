<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.luiseduardo.infrafacil">
    <!-- package="com.example.luiseduardo.autocompletetextview"> -->
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_CONTACTS" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/logo"
        android:label="Infra Fácil"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:windowSoftInputMode="adjustPan">
        <activity android:name="com.example.luiseduardo.infrafacil.Produtos"></activity>
        <activity
            android:name="com.example.luiseduardo.infrafacil.Financas"
            android:label="@string/title_activity_financas"
            android:theme="@style/AppTheme.NoActionBar" />

        <meta-data
            android:name="com.google.android.actions"
            android:resource="@xml/circular_progress_bar" />

        <activity
            android:name="com.example.luiseduardo.infrafacil.MainActivity"
            android:label="Infra - DashBoard"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.luiseduardo.infrafacil.Ordem"
            android:label="Infra - Fácil">
            <intent-filter>
                <action android:name="android.intent.action.VIEW" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.luiseduardo.infrafacil.Cadastro_Clientes"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.VIEW" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.luiseduardo.infrafacil.Status_Ordem"
            android:label="Infra - Descrição Ocor."
            android:windowSoftInputMode="stateHidden">
            <intent-filter>
                <action android:name="android.intent.action.VIEW" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.luiseduardo.infrafacil.Clientes"
            android:label="Infra - Clientes."
            android:windowSoftInputMode="stateHidden">
            <intent-filter>
                <action android:name="android.intent.action.VIEW" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.luiseduardo.infrafacil.NewOrdem"
            android:label="@string/app_name" />


        <meta-data
            android:name="com.google.android.gms.car.application"
            android:resource="@xml/automotive_app_desc" />
        <activity
            android:name="com.example.luiseduardo.infrafacil.Cadastro_Produtos"
            android:label="Adicionar Protutos" />

        <meta-data
            android:name="com.google.android.gms.car.application"
            android:resource="@xml/automotive_app_desc" />
    </application>

</manifest>